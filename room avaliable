    pageEncoding="ISO-8859-1"%>
    <%@include file="header.jsp" %>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<title></title>

<script>
function change(id)
{
	if(id=='1')
		{
			$("#event").val('book');
		}
	else if(id=='2')
		{
		$("#event").val('roomrequest');
		}
	else if(id=='3')
	{
	$("#event").val('hotels');
	}
	else
		{
		$("#event").val('roomrequest');
		}
	
	$("#otherrooms").submit();
}

</script>
</head>
<body>
<form action="HolidaySpot" id="otherrooms">
<table>
<tr>
<td>
The Requested Rooms are available. please Book the Room.
</td>
</tr>
<tr>
<td>
<input type ="button" value="Book" onclick="change('1')">
</td>
</tr>
<tr>
<td>
<input type ="button" value="List Other Rooms" onclick="change('2')">
</td>
</tr>
<tr>
<td>
<input type ="button" value="Change" onclick="change('3')">
</td>
</tr>
<tr>
<td>
<input type ="button" value="Cancel" onclick="change('4')">
</td>
</tr>

</table>
<input type="hidden" name="event" id="event">
<input type="hidden" name="custid" value='<%=request.getParameter("custId") %>'>
</form>
